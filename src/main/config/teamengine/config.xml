<!-- Unpack the *-ctl-scripts.zip archive in the TE_BASE/scripts directory -->
<!-- Add this entry to TE_BASE/config.xml (//organization/standard) -->
<organization>
  <name>OGC</name>
  <standard>
    <name>Conformance Test Suite - GML Documents</name>
    <version>
      <name>${spec-version}</name>
      <suite>
        <namespace-uri>http://www.opengis.net/cite/gml32-data</namespace-uri>
        <prefix>tns</prefix>
        <local-name>ets-gml32-data-${version}</local-name>
        <title>Test suite ets-gml32-data</title>
        <description>Describe scope of testing.</description>
      </suite>
      <revision>
        <name>${version}</name>
        <status>Beta</status>
        <sources>
          <source>gml32-data/${spec-version}/ctl</source>
        </sources>
        <webdir>gml32-data/${spec-version}/site</webdir>
      </revision>
    </version>
  </standard>
</organization>
